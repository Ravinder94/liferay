<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.3.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_3_0.dtd">

<service-builder dependency-injector="ds" package-path="com.softwaresavvyblog.liferay.webservices">
	<author>Ravinder</author>
	<namespace>WebServices</namespace>
	<entity local-service="true" name="Employee" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="empId" primary="true" type="long" id-type="increment"/>

		<!-- Group instance -->

		<column name="empName" type="String" />


	</entity>
</service-builder>